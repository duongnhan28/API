        </nav>

        <!-- partial -->
        <div class="main-panel">
            <div class="content-wrapper">
                <div class="row">

                    <div class="col-lg-12 grid-margin stretch-card">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">List Khoa</h4>
                                <p class="card-description">
                                   <a href="/sinhvien/add">Add Student</a>
                                </p>
                                <div class="table-responsive">
                                    <table class="table table-striped">
                                        <thead>
                                        <tr>
                                            <th>STT</th>
                                            <th>Full Name</th>
                                            <th>Class</th>
                                            <th>Birth-Day</th>
                                            <th>Gender</th>
                                            <th>Department</th>
                                            <th>Edit</th>
                                            <th>Update</th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                        <c:forEach items="${sinhvien}" var="sinhvien"   varStatus="i" begin="0" end="100">
                                        <tr>

                                            <td >${i.index+1}</td>
                                            <td>${sinhvien.hoten}</td>
                                            <td>${sinhvien.lop.tenlop}</td>
                                            <td>${sinhvien.ngaysinh}</td>
                                            <td>${sinhvien.gioitinh}</td>
                                            <td>${sinhvien.lop.khoa.tenkhoa}</td>
                                            <td><a type="submit" href="/sinhvien/edit?masv=${sinhvien.masv}">Edit </a></td>
                                            <td><a type="submit" href="/deleleSinhVien?masv=${sinhvien.masv}">Delete </a></td>

                                        </tr>
                                        </c:forEach>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
            <!-- content-wrapper ends -->
            <!-- partial:../../partials/_footer.html -->
            <footer class="footer">
                <div class="d-sm-flex justify-content-center justify-content-sm-between">
